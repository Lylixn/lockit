{
  "name": "lockit",
  "version": "1.0.0",
  "description": "",
  "main": "js/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./js/main.js",
    "package": "npx electron-packager . lockit",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder"
