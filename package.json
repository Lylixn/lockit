  "build": {
    "appId": "fr.lylixn.lockit",
    "win": {
      "target": ["msi", "nsis"],
      "icon": "icon.png"
    },
    "mac": {
      "icon": "icon.png"
    }
  },
  "keywords": [],
  "author": "Lylixn",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "eslint": "^7.32.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "zxcvbn": "^4.4.2"
  }
}
